#Variables
multi1, multi2 = 3, 5
loop, total = 1000, 0
multiArray = []

#Main Code
if __name__ == "__main__":

    #0(n)
    for i in range(1, loop):
        if i%3 == 0 or i%5 == 0:
            multiArray.append(i)
        else:
            continue

    #0(n)
    for i in range(len(multiArray)):
        total+= multiArray[i]

    print(multiArray, total)
